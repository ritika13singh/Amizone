{
    "name":"budgety",
    "version":"1.0",
    "manifest_version":2,
    "browser_action":{
        "default_popup":"popup.html"
    },
    "options_page": "options.html",
    "background":{
        "scripts":["eventPage.js"],
        "persistent":false
    },
    "permissions":[
        "storage",
        "notifications",
        "contextMenus"
    ]
}